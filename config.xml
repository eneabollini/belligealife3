	id          = "com.enea.belligeaLIFE"
	versionCode = "10" 
	version     = "1.0.0" >
    <name>belligeaLIFE</name>
    <description>
        L'app dedicata a Bellaria Igea Marina
    </description>
    <author email="info@eneabollini.it" href="http://www.belligeanews.it">
        belligeaNews
    </author>



	<gap:platform name="ios" />
